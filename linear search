#include<stdio.h>
int main()
{
	int A[10],i,n,flag=0,key;
	printf("enter the size of array");
	scanf("%d", &n);
	printf("enter the number of arrays");
	for (i=0;i<n;i++)
	scanf("%d", &A[i]);
	printf("enter the key");
	scanf("%d", &key);
	
	for (i=0;i<n;i++)
	{
		if (A[i]==key)
		{
			flag=1;
		break;
			
		}
	}
	if (flag==1)
      printf("the elements in list");
    else
printf("the elements not in a list ");
return 0;
}
